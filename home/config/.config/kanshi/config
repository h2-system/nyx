  exec hyprctl dispatch moveworkspacetomonitor 2 "Samsung Electric Company SAMSUNG 0x01000E00"
  exec hyprctl dispatch moveworkspacetomonitor 3 "Dell Inc. DELL P3424WE BFY96T3"
}

